-@suggestions.sort! { |a,b| a.name <=> b.name }
-if textlength>0
  #suggestions
    -unless @suggestions.nil?
      - if @suggestions.length > 0
        - @suggestions.each do |suggestion|
          %a.suggestion_link{:onclick=>'select_material(this)'}
            .suggestion
              = suggestion.name
-else
  %div